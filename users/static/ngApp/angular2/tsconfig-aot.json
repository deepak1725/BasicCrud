{
    "compileOnSave": false,
    "compilerOptions": {
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es5",
      "module": "es2015",
      "noImplicitAny":true,
      "suppressImplicitAnyIndexErrors":true,
      "typeRoots": [
        "./node_modules/@types"
      ],
      "lib": [
        "es2016",
        "dom"
      ]
    },
    "files": [
        "src/app/app.module.ts",
        "src/main.ts"
      ],
    
    "angularCompilerOptions": {
        "genDir": "aot",
        "skipMetadataEmit" : true
    }
}
  